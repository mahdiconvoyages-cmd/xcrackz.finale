{
  "expo": {
    "name": "xCrackz",
    "slug": "xcrackz-mobile",
    "version": "6.0.2",
    "scheme": "finality",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b1220"
    },
    "owner": "xcrackz123-2",
    "assetBundlePatterns": [
      "assets/images/**/*",
      "assets/vehicles/**/*",
      "assets/icon.png",
      "assets/adaptive-icon.png",
      "assets/splash.png"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.finality.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "xCrackz a besoin d'accéder à votre position pour le suivi GPS des missions.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "xCrackz a besoin d'accéder à votre position en arrière-plan pour suivre vos missions en temps réel.",
        "NSCameraUsageDescription": "xCrackz a besoin d'accéder à votre caméra pour prendre des photos lors des inspections.",
        "NSPhotoLibraryUsageDescription": "xCrackz a besoin d'accéder à vos photos pour les rapports d'inspection.",
        "NSMicrophoneUsageDescription": "xCrackz a besoin d'accéder au microphone pour les notes vocales.",
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "com.finality.app",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0b1220"
      },
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResourcesInReleaseBuilds": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "finality"
            },
            {
              "scheme": "https",
              "host": "finality.app",
              "pathPrefix": "/mission"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-location",
      "expo-camera",
      "expo-notifications",
      "expo-font",
      "expo-secure-store",
      "expo-task-manager",
      "react-native-maps"
    ],
    "updates": {
      "url": "https://u.expo.dev/ba5fcd57-97ee-4ed7-93ff-9c79b7c6e2e9",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": "4.6.0",
    "extra": {
      "eas": {
